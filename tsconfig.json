{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es6","es5","dom"],
    "types": ["reflect-metadata","node"],
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators":true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "declaration": false,
    "outDir": "dist"
  },
  "exclude": [
    "node_modules"
  ],
}