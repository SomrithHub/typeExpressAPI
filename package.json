{
  "name": "studentms",
  "version": "1.0.0",
  "description": "express scaffolding project",
  "main": "src/index.ts",
  "scripts": {
    "serve": "ts-node src/index.ts",
    "dev:app": "nodemon --config \"./util/nodemon.json\"/",
    "dev": "tsc && nodemon dist/index.js",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "yarn build",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "inits": "tsc --init",
    "type": "typeorm init --name database --database mysql",
    "db:start": "ts-node src/database/src/index.ts",
    "db:migrate": "typeorm migration:run",
    "db:rollback": "typeorm migration:revert",
    "test": "typeorm migration:create -n Admin",
    "test2": "typeorm migration:generate"
  },
  "author": "somrith",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "nodemon": "^1.17.4",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.8.1-alpha.2",
    "sqlite3": "^4.1.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/method-override": "^0.0.31",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.7.7",
    "@types/nodemon": "^1.19.0",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sqlite3": "^3.1.6"
  }
}
